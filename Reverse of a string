import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String ans=sc.nextLine();
        ans=ans.toLowerCase();
        String res="";
        for(int i=ans.length()-1;i>=0;i--){
            res=res+ans.charAt(i);
        }
        System.out.print(res);
    }
}
