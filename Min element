import java.util.*;
public class Main{
    public static void main(String args[]){
       int a[]={4, 2, 6, 5};
       int n=a.length;
        int min=a[0];
        for(int i=0;i < n;i++){
            if(a[i] < min){
                min=a[i];
            }
        }
        System.out.print(min);
    }
}
