import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int result=0;
    while(n>0){
        int rem=n%10;
        result=result*10+rem;
        n=n/10;
        
    }
    System.out.print("Reversed Number is "+result);
    }
}
