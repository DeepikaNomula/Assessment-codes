import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int ans=0;
        for(int i=1;i<=n/2;i++){
            if(n%i==0){
                ans+=i;
            }
        }
        if(n==ans){
            System.out.println(ans+" is a Perfect Number");
        }
        else{
            System.out.println(ans+" is not a Perfect Number");
        }
    } 
}
